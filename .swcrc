{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "decoratorMetadata": true,
      "legacyDecorator": true
    },
    "target": "es2020",
    "keepClassNames": true
  },
  "module": {
    "type": "commonjs",
    "strict": true
  },
  "sourceMaps": false
}
